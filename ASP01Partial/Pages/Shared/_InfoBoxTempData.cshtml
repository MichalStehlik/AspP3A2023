@using ASP01Partial.Helpers;
@model string
@{
    List<TempMessage> messages = TempData.GetMessages(this.Model);
}
@if (messages != default)
{
    <div class="jumbotron">
        @foreach (var message in messages)
        {
            <div class="alert alert-@message.Type.ToString().ToLower() alert-dismissible fade show" role="alert">
                <span>@message.Text</span>
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
        }
    </div>
}